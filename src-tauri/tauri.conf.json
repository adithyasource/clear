{
  "build": {
    "beforeDevCommand": "pnpm run dev",
    "beforeBuildCommand": "pnpm run build",
    "beforeBundleCommand": ".\\scripts\\beforeBundle.bat",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": [
      "nsis",
      "msi"
    ],
    "icon": [
      "icons/128x128.png",
      "icons/icon.ico"
    ]
  },
  "productName": "clear",
  "mainBinaryName": "clear",
  "version": "1.1.0",
  "identifier": "com.adithya.clear",
  "plugins": {},
  "app": {
    "withGlobalTauri": false,
    "security": {
      "assetProtocol": {
        "scope": [
          "**"
        ],
        "enable": true
      }
    },
    "windows": [
      {
        "title": "clear",
        "width": 1220,
        "height": 760,
        "dragDropEnabled": false,
        "minHeight": 390,
        "minWidth": 640,
        "theme": "Dark",
        "visible": false
      }
    ]
  }
}
